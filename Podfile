# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'SopraSteria' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SopraSteria
   pod 'SnapKit', '~> 4.0'
   pod 'Siesta',  '~> 1.0'
   pod 'RxSwift', '~> 4.0'
   pod 'RxCocoa', '~> 4.0'
   pod 'RealmSwift'
   

   target 'SopraSteriaTests' do
       inherit! :search_paths
       # Pods for testing
       pod 'RealmSwift'
       pod 'SwiftyJSON'
       pod 'Siesta', '~> 1.0'
   end
  

end
